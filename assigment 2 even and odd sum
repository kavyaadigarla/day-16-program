#include<stdio.h>
main()
{
	int n,m,i=1,sum1=0,sum2=0;
	printf("enter the value of n and m\n");
	scanf("%d%d",&n,&m);
	while(i<=n)
	{
		if(i%m==0)
		{
			printf("%d\n",i);
			if(i%2==0)
			{
				sum1=sum1+i;
			}
			else
			{
				sum2=sum2+i;
			}
		    }
		i++;
	}
	printf("even sum=%d\t",sum1);
	printf("odd sum=%d",sum2);
}
